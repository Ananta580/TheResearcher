@model TheResearcher.Models.tblPaper

@{
    ViewBag.Title = "Details";
}
<div class="container">
    <dl class="dl-horizontal">


  

        <dd>
          <b>  @Html.DisplayFor(model => model.Title)</b>
        </dd>

 
  

        <dd>
            @Html.DisplayFor(model => model.Date)
        </dd>

   

        <dd>
            <iframe src="~/PDFS/@Model.Pdf" width="100%" height="1000"></iframe>
        </dd>


        <dd>

            @Html.Action("ForumComment", "Paper", new { id = Model.PaperID })
            @Html.Action("ForumView", "Paper", new { id = Model.PaperID })
        </dd>
    </dl>
</div>

